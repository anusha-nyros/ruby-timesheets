<%= render :partial => "menu"%>

<%= simple_form_for(@projecttype, :html => {:multipart => true ,:class => 'form-horizontal'}) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :projecttype%>
    <%= f.input :usage, input_html: {rows: '5', class: 'span9'} %>
  </div>

  <div class="form-actions">
    <%= link_to 'Back', projecttypes_path , class: 'btn'%>
    <%= f.button :submit ,class: 'btn btn-primary' %>
  </div>
<% end %>
